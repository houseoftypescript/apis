{
  "name": "apis",
  "version": "0.0.1",
  "description": "APIs",
  "main": "dist/server.js",
  "repository": "git@github.com:houseoftypescript/apis.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "scripts": {
    "prebuild": "rimraf dist && yarn docs:swagger:html",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger.json > ./docs/swagger.yaml",
    "docs:swagger:html": "redoc-cli build ./docs/swagger.json --output ./public/index.html --title \"HOM APIs\"",
    "prepare": "husky install || true",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "prisma:migrate": "prisma format && prisma generate && prisma migrate dev --name init",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "axios": "1.3.4",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "morgan": "1.10.0",
    "pino": "8.11.0",
    "pino-pretty": "10.0.0",
    "redis": "4.6.5",
    "tsoa": "5.1.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.4.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.0",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@webpack-cli/generators": "3.0.1",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "eslint": "8.36.0",
    "http-errors": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "json2yaml": "1.1.0",
    "nodemon": "2.0.21",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "prisma": "4.11.0",
    "redoc-cli": "0.13.21",
    "rimraf": "4.4.0",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
